<% LanguageManager = require '../lib/language-manager' %>

<div class="languages">
  <% for code, {label} of LanguageManager.current?.translations: %>
    <div class="language">
      <button name="language" value="<%= code %>" <% if code is LanguageManager.current?.code: %>class="active"<% end %>><%= label %></button>
    </div>
  <% end %>
</div>
